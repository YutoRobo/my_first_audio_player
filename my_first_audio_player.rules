
(rules PCB my_first_audio_player
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (eu.mihosoft.freerouting.autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 6751)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.3)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.8)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-Bold" "Via[0-1]_800:400_um" Bold
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (via_rule
    Bold "Via[0-1]_800:400_um-Bold"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "Net-(C1-Pad1)" /+VBAT "Net-(C4-Pad1)" "Net-(C6-Pad2)" "Net-(C6-Pad1)" "Net-(C8-Pad1)" "Net-(C18-Pad2)" "Net-(C18-Pad1)"
    /+VUSB "Net-(F1-Pad2)" "Net-(J2-Pad4)" "Net-(J2-Pad3)" "Net-(J2-Pad2)" /DATA /BCK /LRCK
    /SCK "Net-(Q1-Pad1)" "Net-(Q1-Pad2)" "Net-(R1-Pad1)" "Net-(R3-Pad2)" "Net-(S1-Pad3)" "Net-(U1-Pad4)" /ZEROL
    /ZEROR /LRLK "Net-(U5-Pad4)" "/C1L_1" "/C1L_2" "/C2L_1" "/C3L_2" "/C3L_1"
    "/C1R_2" "/C1R_1" "/C2R_1" "/C3R_2" "/C3R_1" "/R2L_1" "/R1L_1" "/R2R_1"
    "/R1R_1"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class Bold
    GND /+3V3 /+5V "/-5V" "/L_OUT" "/R_OUT"
    (clearance_class Bold)
    (via_rule Bold)
    (rule
      (width 400.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)